{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "noEmit": true,
    "outDir": "./dist/renderer"
  },
  "include": ["src/renderer/**/*", "src/shared/**/*"]
}
